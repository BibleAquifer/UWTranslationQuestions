    }
  },
  {
    "content_id": 674861,
    "reference_id": 120398,
    "version": "1.0.1",
    "title": "1 Timotius 3:15",
    "media_type": "Text",
    "index_reference": "54003015",
    "language": "ind",
    "review_level": "None",
    "content": "<p><strong>Apakah yang dimaksud dengan keluarga Allah?</strong></p><p>Keluarga Allah adalah jemaat Allah yang hidup</p>",
    "associations": {
      "passage": [
        {
          "start_ref": "54003015",
          "start_ref_usfm": "1TI 3:15",
          "end_ref": "54003015",
          "end_ref_usfm": "1TI 3:15"
        }
