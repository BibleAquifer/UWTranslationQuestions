  {
    "content_id": 676899,
    "reference_id": 120389,
    "version": "1.0.0",
    "title": "१ तिमोथी 3:1",
    "media_type": "Text",
    "index_reference": "54003001",
    "language": "nep",
    "content": "<p><strong>बिशपको काम कस्तो प्रकारको काम हो?</strong></p><p>बिशपको काम असल काम हो।</p>",
    "associations": {
      "passage": [
        {
          "start_ref": "54003001",
          "start_ref_usfm": "1TI 3:1",
          "end_ref": "54003001",
