    "reference_id": 126351,
    "version": "1.0.0",
    "title": "Tiago 2.18",
    "media_type": "Text",
    "index_reference": "59002018",
    "language": "por",
    "content": "<p><strong>Como Tiago diz que devemos mostrar nossa fé?</strong></p><p>Tiago diz que devemos mostrar nossa fé por nossas obras.</p>",
    "associations": {
      "passage": [
        {
          "start_ref": "59002018",
          "start_ref_usfm": "JAS 2:18",
          "end_ref": "59002018",
          "end_ref_usfm": "JAS 2:18"
        }
      ],
      "resource": []
    }
  },
