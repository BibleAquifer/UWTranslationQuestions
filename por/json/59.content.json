    "reference_id": 126345,
    "version": "1.0.0",
    "title": "Tiago 2.8",
    "media_type": "Text",
    "index_reference": "59002008",
    "language": "por",
    "content": "<p><strong>Qual é a lei do Reino das escrituras?</strong></p><p>A lei real é: “Amarás o teu próximo como a ti mesmo”.</p>",
    "associations": {
      "passage": [
        {
          "start_ref": "59002008",
          "start_ref_usfm": "JAS 2:8",
          "end_ref": "59002008",
          "end_ref_usfm": "JAS 2:8"
        }
      ],
      "resource": []
    }
  },
  {
    "content_id": 580225,
