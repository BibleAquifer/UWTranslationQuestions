    "version": "1.0.0",
    "title": "Jonas 3.6 (#1)",
    "media_type": "Text",
    "index_reference": "32003006",
    "language": "fra",
    "content": "<p><span><strong>À qui la nouvelle est-elle parvenue&nbsp;?</strong></span></p><p><span>La nouvelle est parvenue au roi de Ninive.</span></p>",
    "associations": {
      "passage": [
        {
          "start_ref": "32003006",
          "start_ref_usfm": "JON 3:6",
          "end_ref": "32003006",
          "end_ref_usfm": "JON 3:6"
