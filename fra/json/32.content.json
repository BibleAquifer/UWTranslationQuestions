    "version": "1.0.0",
    "title": "Jonas 3.7 (#1)",
    "media_type": "Text",
    "index_reference": "32003007",
    "language": "fra",
    "content": "<p><span><strong>Par ordre de qui la publication a-t-elle été faite dans Ninive&nbsp;?</strong></span></p><p><span>Par ordre du roi et de ses grands.</span></p>",
    "associations": {
      "passage": [
        {
          "start_ref": "32003007",
          "start_ref_usfm": "JON 3:7",
          "end_ref": "32003007",
          "end_ref_usfm": "JON 3:7"
        }
