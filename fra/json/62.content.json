    "reference_id": 119326,
    "title": "1 Jean 1.7",
    "media_type": "Text",
    "index_reference": "62001007",
    "language": "fra",
    "content": "<p><strong>Qu&apos;est-ce qui purifie de tous les péchés ceux qui marchent dans la lumière ?</strong></p><p>Le sang de Jésus les purifie de tout péché.</p>",
    "associations": {
      "passage": [
        {
          "start_ref": "62001007",
          "start_ref_usfm": "1JN 1:7",
          "end_ref": "62001007",
          "end_ref_usfm": "1JN 1:7"
        }
      ],
      "resource": []
    }
  },
  {
